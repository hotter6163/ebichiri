{
  "name": "@ebichiri/expo",
  "version": "0.1.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "dev": "expo start --ios",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "build:simulator": "dotenv -- eas build --profile=development --platform=ios --no-wait",
    "lint": "eslint .",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/metro-config": "^0.17.6",
    "@react-native-community/slider": "4.4.2",
    "@shopify/flash-list": "1.6.3",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.8.7",
    "@trpc/client": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "expo": "^50.0.14",
    "expo-apple-authentication": "~6.3.0",
    "expo-camera": "~14.1.1",
    "expo-constants": "~15.4.5",
    "expo-crypto": "^12.8.1",
    "expo-dev-client": "~3.3.11",
    "expo-image": "~1.10.6",
    "expo-linking": "~6.2.2",
    "expo-location": "~16.5.5",
    "expo-media-library": "~15.9.1",
    "expo-router": "3.4.8",
    "expo-secure-store": "~12.8.1",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "nativewind": "^4.0.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.1",
    "react-native-reanimated": "~3.6.3",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-url-polyfill": "^1.3.0",
    "superjson": "2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/runtime": "^7.23.2",
    "@ebichiri/api": "workspace:^0.1.0",
    "@ebichiri/eslint-config": "workspace:^0.2.0",
    "@ebichiri/prettier-config": "workspace:^0.1.0",
    "@ebichiri/tailwind-config": "workspace:^0.1.0",
    "@ebichiri/tsconfig": "workspace:^0.1.0",
    "@expo/config-plugins": "^7.8.4",
    "@types/babel__core": "^7.20.4",
    "@types/react": "^18.2.42",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.53.0",
    "prettier": "^3.1.0",
    "tailwindcss": "3.3.5",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@ebichiri/eslint-config/base",
      "@ebichiri/eslint-config/react"
    ],
    "ignorePatterns": [
      "expo-plugins/**"
    ],
    "rules": {
      "jsx-a11y/alt-text": "off"
    }
  },
  "prettier": "@ebichiri/prettier-config"
}
